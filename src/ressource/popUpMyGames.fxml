<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.effect.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>

<VBox styleClass="small_popUp" stylesheets="@popUp.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.PopUpController">
  	<children>
		<Label styleClass="label_game" text="Voulez-vous reprendre une partie ?" />
		<HBox styleClass="button_container_popUp">
			<children>
				<Button  fx:id="button" mnemonicParsing="false" styleClass="blue_tall_button" onAction="#closePopUp" text="Refuser" />
				<Button fx:id="button" mnemonicParsing="false" styleClass="blue_tall_button" onAction="#goToMyGames" text="Accepter" />
			</children>
		</HBox>
	</children>
</VBox>
